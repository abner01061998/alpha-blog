<table>
    <thead>
        <th>#</th>
        <th>Title</th>
        <th>Description</th>
        <th>Actions</th>
    </thead>
    <tbody>
        <% @articles.each do |article| %>
            <tr>
                <td><%= article.id %></td>
                <td><%= article.title %></td>
                <td><%= article.description %></td>
                <td><%= "path_to" %></td>
            </tr>
        <% end %>
    </tbody>
</table>

